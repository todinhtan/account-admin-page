              <div class="form-group col-md-12">
                <label for="citizenIdNumber">Citizen ID Number</label>
                <input type="text" id="citizenIdNumber" name="authorizationData[citizenIdNumber]" class="form-control" value="">
              </div>
              <div class="form-group col-md-12">
                <label for="dob">Date of birth</label>
                <input type="text" id="dob" name="authorizationData[dobString]" class="form-control" value="">
