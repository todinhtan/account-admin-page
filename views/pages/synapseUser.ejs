<!-- Optionally, you can add Slimscroll and FastClick plugins.
     Both of these plugins are recommended to enhance the
     user experience. -->
  <script>
  $('.content').on('click', '.open-walletId-modal', function(e) {
    var walletId = $(this).text();
    var targetId = $(this).data('target');
    var _modal = $(targetId);
    $.ajax({
      url: '/wallet/' + walletId + '/vba/ajax',
      type: 'GET',
      success: function(data) {
        _modal.find('#id_doc_img').attr('src', data.idDocUri);
        _modal.find('.vba-data pre').html(JSON.stringify(data, undefined, 2));
      },
      error: function(error) {
        console.log(error);
      }
    })
  });

  $('.content').on('click', '.open-userId-modal', function(e) {
    var userId = $(this).text();
    var targetId = $(this).data('target');
    var _modal = $(targetId);
    $.ajax({
      url: '/synapse/user/' + userId,
      type: 'GET',
      success: function(data) {
        if (data.documents && data.documents.length) {
          _modal.find('.physical_docs pre').html(JSON.stringify(data.documents[0].physical_docs, undefined, 2));
          _modal.find('.social_docs pre').html(JSON.stringify(data.documents[0].social_docs, undefined, 2));
        }
      },
      error: function(error) {
        console.log(error);
      }
    })
  })

  $('.content').on('submit', '.form-doc-type', function(e) {
    var _self = $(this);
    var _docType = _self.find('[name=docType]').val();
    if (_docType === 'authorization') {
      var _modal = $('#authorizeDoc-modal');
      var _walletId = _self.find('[name=walletId]').val();
      var _userId = _self.find('[name=userId]').val();
      _modal.find('[name=walletId]').val(_walletId);
      _modal.find('[name=userId]').val(_userId);
      $.ajax({
        url: '/vba/wallet/' + _walletId + '/pinyin',
        type: 'GET',
        success: function(data) {
          console.log(data.address.street1);
          _modal.find('#id_doc_img').attr('src', data.idDocUri);
          _modal.find('[name="authorizationData[idDoc]"]').val(data.idDocUri);

          // fill data
          _modal.find('[name="authorizationData[fullName]"]').val(data.nameEn || data.nameCn);
          _modal.find('[name="authorizationData[sex]"]').val(data.entityType);
          _modal.find('[name="authorizationData[ethnicity]"]').val('');
          _modal.find('[name="authorizationData[citizenIdNumber]"]').val(data.idNumber);
          _modal.find('[name="authorizationData[dobString]"]').val(data.dobString);
          _modal.find('[name="authorizationData[address][street1]"]').val(data.address.street1);
          _modal.find('[name="authorizationData[address][street2]"]').val(data.address.street2);
          _modal.find('[name="authorizationData[address][city]"]').val(data.address.city);
          _modal.find('[name="authorizationData[address][state]"]').val(data.address.state);
          _modal.find('[name="authorizationData[address][postalCode]"]').val(data.address.postalCode);
          _modal.find('[name="authorizationData[address][country]"]').val(data.address.country);

          _modal.modal('show');
        },
        error: function(error) {
          console.log(error);
        },
      })
      return false;
    } else if (_docType === 'idDoc') {
      var _modal = $('#idDoc-modal');
      var _walletId = _self.find('[name=walletId]').val();
      var _userId = _self.find('[name=userId]').val();
      _modal.find('[name=walletId]').val(_walletId);
      _modal.find('[name=userId]').val(_userId);
      $.ajax({
        url: '/wallet/' + _walletId + '/idDoc',
        type: 'GET',
        success: function(data) {
          _modal.find('[name=idDoc]').val(data && data.idDoc ? data.idDoc : '');
          var uri = data && data.docUri ? data.docUri : 'https://via.placeholder.com/300x225';
          _modal.find('#id_doc_img').attr('src', uri);

          _modal.modal('show');
        },
        error: function(error) {
          console.log(error);
        },
      })
      return false;
    } else if (_docType === 'coiDoc') {
      var _modal = $('#coiDoc-modal');
      var _walletId = _self.find('[name=walletId]').val();
      var _userId = _self.find('[name=userId]').val();
      _modal.find('[name=walletId]').val(_walletId);
      _modal.find('[name=userId]').val(_userId);
      $.ajax({
        url: '/wallet/' + _walletId + '/coiDoc',
        type: 'GET',
        success: function(data) {
          _modal.find('[name=coiDoc]').val(data && data.coiDoc ? data.coiDoc : '');
          var uri = data && data.docUri ? data.docUri : 'https://via.placeholder.com/300x225';
          _modal.find('#id_doc_img').attr('src', uri);

          _modal.modal('show');
        },
        error: function(error) {
          console.log(error);
        },
      })
      return false;
    } else if (_docType === 'amz') {
      var _modal = $('#amz-modal');
      var _walletId = _self.find('[name=walletId]').val();
      var _userId = _self.find('[name=userId]').val();
      _modal.find('[name=walletId]').val(_walletId);
      _modal.find('[name=userId]').val(_userId);
      $.ajax({
        url: '/wallet/' + _walletId + '/merchants',
        type: 'GET',
        success: function(data) {
          if (data && data.length) {
            _modal.find('.amz-link').text(data[0].merchantId ? 'https://www.amazon.com/s?merchant=' + data[0].merchantId : 'empty');
            _modal.find('.amz-link').attr('href', data[0].merchantId ? 'https://www.amazon.com/s?merchant=' + data[0].merchantId : '#');
            _modal.find('[name="merchantId"]').val(data[0].merchantId ? data[0].merchantId : 'empty');
          }

          _modal.modal('show');
        },
        error: function(error) {
          console.log(error);
        },
      })
      return false;
    } else if (_docType === 'basic' || _docType === 'company_basic') {
      var _modal = $('#basic-modal');
      var _walletId = _self.find('[name=walletId]').val();
      var _userId = _self.find('[name=userId]').val();

      if (_docType === 'basic') _modal.find('.modal-title').text('Synapse individual information');
      else _modal.find('.modal-title').text('Synapse company information');

      _modal.find('[name=walletId]').val(_walletId);
      _modal.find('[name=userId]').val(_userId);
      _modal.find('[name=docType]').val(_docType);

      _modal.find('[name="userData[email]"]').val(_self.find('.' + _docType + '.email').val());
      _modal.find('[name="userData[phone_number]"]').val(_self.find('.' + _docType + '.phone_number').val());
      _modal.find('[name="userData[ip]"]').val(_self.find('.' + _docType + '.ip').val());
      _modal.find('[name="userData[address_street]"]').val(_self.find('.' + _docType + '.address_street').val());
      _modal.find('[name="userData[address_city]"]').val(_self.find('.' + _docType + '.address_city').val());
      _modal.find('[name="userData[address_subdivision]"]').val(_self.find('.' + _docType + '.address_subdivision').val());
      _modal.find('[name="userData[address_postal_code]"]').val(_self.find('.' + _docType + '.address_postal_code').val());
      _modal.find('[name="userData[address_country_code]"]').val(_self.find('.' + _docType + '.address_country_code').val());
      _modal.find('[name="userData[day]"]').val(_self.find('.' + _docType + '.day').val());
      _modal.find('[name="userData[month]"]').val(_self.find('.' + _docType + '.month').val());
      _modal.find('[name="userData[year]"]').val(_self.find('.' + _docType + '.year').val());
      
      _modal.modal('show');
      return false;
    }
  });

  $('.form-ajax').on('submit', function(e) {
    e.preventDefault();
    var _self = $(this);
    var _target = _self.data('target');
    var _formTarget = $('[data-form='+_target+']');
    _self.find('.message').hide();
    $.ajax({
      url: _self.attr('action'),
      type: 'POST',
      data: _self.serialize(),
      success: function(data) {
        console.log(data);
        if (data) {
          _formTarget.find('[name='+_target+']').val(data.doc);
          _formTarget.find('#id_doc_img').attr('src', data.docUri ? data.docUri : 'https://via.placeholder.com/300x225');
          _self.find('.has-success').fadeIn();
        } else _self.find('.has-error').fadeIn();
      },
      error: function(error) {
        console.log(error);
      },
    });
  })

  $('.form-merchant-ajax').on('submit', function(e) {
    e.preventDefault();
    var _self = $(this);
    var _target = 'amz';
    var _formTarget = $('[data-form='+_target+']');
    _self.find('.message').hide();
    $.ajax({
      url: _self.attr('action'),
      type: 'POST',
      data: _self.serialize(),
      success: function(data) {
        console.log(data);
        if (data) {
          _formTarget.find('.amz-link').text('https://www.amazon.com/s?merchant=' + data);
          _formTarget.find('.amz-link').attr('href', 'https://www.amazon.com/s?merchant=' + data);
          _self.find('.has-success').fadeIn();
        } else _self.find('.has-error').fadeIn();
      },
      error: function(error) {
        console.log(error);
      },
    });
  })
  </script>
</body>
</html>