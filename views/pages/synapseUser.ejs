              <div class="form-group col-md-6 col-sm-12">
                <label for="street2">Street2</label>
                <input type="text" id="street2" name="authorizationData[address][street2]" class="form-control" value="">
              </div>
              <div class="form-group col-md-6 col-sm-12">
                <label for="city">City</label>
                <input type="text" id="city" name="authorizationData[address][city]" class="form-control" value="">
              </div>
              <div class="form-group col-md-6 col-sm-12">
                <label for="state">State</label>
                <input type="text" id="state" name="authorizationData[address][state]" class="form-control" value="">
              </div>
              <div class="form-group col-md-6 col-sm-12">
                <label for="postalCode">Postal Code</label>
                <input type="text" id="postalCode" name="authorizationData[address][postalCode]" class="form-control" value="">
              </div>
              <div class="form-group col-md-6 col-sm-12">
                <label for="country">Country</label>
                <input type="text" id="country" name="authorizationData[address][country]" class="form-control" value="">
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </form>
      </div>
      <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
  </div>

