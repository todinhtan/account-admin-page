<% var page_title = 'EPIAPI Admin | Unauthorized VBA'; %>
<!DOCTYPE html>
<html>
<head>
  <% include ../partials/head %>
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">
  <% include ../partials/header %>
  <% include ../partials/left-sidebar %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>VBA status updates
        <small>Total <%= listSynapseUsers.total %> results</small>
      </h1>
    </section>

    <!-- Main content -->
    <section class="content container-fluid">
      <% include ../partials/message-container %>
      <div class="row">
        <% if (listSynapseUsers.items.length > 0) { %>
        <div class="col-md-12">
          <%- include('../includes/_table/synapseUser', { listSynapseUsers: listSynapseUsers, useDataTablePaging: true }) %>
        </div>
        <% } %>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <div class="modal fade" id="walletId-modal">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title">VBA verification data & Id document</h4>
        </div>
        <div class="modal-body">
          <div class="row">
            <img id="id_doc_img" style="max-width: 300px; display: block; margin: 0 auto;" src="https://via.placeholder.com/300x225" alt="">
          </div>
          <hr>
          <div class="row">
            <h4 style="text-align: center;">VBA verification data</h4>
            <div class="vba-data">
              <pre></pre>
            </div>
          </div>
        </div>
      </div>
      <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
  </div>

  <div class="modal fade" id="userId-modal">
